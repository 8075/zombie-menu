		if (= $editing 1) [ 
		entfind particles 4 309 10 * 
		entfind teleport (- $count 1) -1 
		entfind teleport $count -1
		entfind teledest * (- $tcount 1) 1 
		entfind teledest * $count 1
		delent 
		] [ 
		edittoggle 
		entfind particles 4 309 10 * 
		entfind teleport (- $count 1) -1 
		entfind teleport $count -1
		entfind teledest * (- $tcount 1) 1 
		entfind teledest * $count 1
		delent 
		edittoggle 
		]	
		
echo "All portals have been cleared." 
]
