zOMBIEKEYS = [

  bind "J" [$zQX "^f0Grrrrrraaaaiiiinnnsssss!! ~~D8"]


  bind "K" [edittoggle; edittoggle; kill; if (= $zKILL 0) [$zQX "^f0oh noes! a zombie bit me!! XO"; zKILL = 1; sleep 3000 [zKILL = 0]]]

  bind "L" [$zQX "^f0-.-"]


  bind "M" [if (!= $soundvol 0) [zBKSOUNDVOL = $soundvol; soundvol 0; $zQE (concatword "^f0" $zWL "Global soundvolume muted! (^f5/soundvol 0^f0)" $zWR)] [soundvol $zBKSOUNDVOL; $zQE (concatword "^f0" $zWL "Global soundvolume back to " $zBKSOUNDVOL "! (^f5/soundvol " $zBKSOUNDVOL "^f0)" $zWR)]]



  bind "C"  [if (= $zPORTALS 1) [gridpower 4; zPORTALS = 0; showgui zombie; sleep 5 [cleargui; $zQE (concatword "^f0" $zWL "Portal-Mode  ^f1offline^f0  " $zFM "! o.o (^f0/zPORTALS = 0^f0)" $zWR)]] [gridpower 0.5; zPORTALS = 1; showgui zombie; sleep 5 [cleargui; $zQE (concatword "^f0" $zWL "Portal-Mode  ^f6online^f0  " $zFM "! ^f2o^f0.^f1O^f0 (^f0/zPORTALS = 1^f0)" $zWR)]]]
]
